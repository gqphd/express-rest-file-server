{
  "name": "express-rest-file-server",
  "version": "0.0.0-development",
  "description": "CRUD operations with files storing in-memory or disk",
  "keywords": [
    "node",
    "express",
    "crud",
    "rest",
    "multer",
    "files",
    "server"
  ],
  "author": "Francisco Calle Moreno",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/bitIO/express-rest-file-server.git"
  },
  "bugs": {
    "url": "https://github.com/bitIO/express-rest-file-server/issues"
  },
  "homepage": "https://github.com/bitIO/express-rest-file-server/",
  "engines": {
    "node": ">=6"
  },
  "main": "lib/index.js",
  "bin": "./bin/index.js",
  "scripts": {
    "build": "babel --copy-files --out-dir lib --ignore *.test.js src",
    "commit": "git-cz",
    "prebuild": "rimraf lib",
    "prepublishOnly": "pkg-ok",
    "start": "babel-node src/index.js -v",
    "start:disk": "babel-node src/index.js --storageType=disk",
    "start:disk_gq": "babel-node src/index.js --storageType=disk --storagePath=D:\\Tmp",
    "start:prod": "node lib/index.js",
    "test": "echo \"Error: no test specified\" && exit 0",
    "semantic-release": "semantic-release",
    "travis-deploy-once": "travis-deploy-once"
  },
  "dependencies": {
    "body-parser": "^1.18.2",
    "cors": "^2.8.4",
    "express": "^4.16.3",
    "multer": "^1.3.0",
    "node-getopt": "^0.3.2",
    "winston": "^2.4.2"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-stage-0": "^6.24.1",
    "commitizen": "^2.9.6",
    "cz-conventional-changelog": "^2.1.0",
    "eslint": "^4.19.1",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-plugin-import": "^2.11.0",
    "pkg-ok": "^2.2.0",
    "rimraf": "^2.6.2",
    "semantic-release": "^15.3.1",
    "travis-deploy-once": "^5.0.0"
  },
  "babel": {
    "plugins": [],
    "presets": [
      "env",
      "stage-0"
    ]
  },
  "eslintConfig": {
    "extends": "airbnb-base",
    "rules": {
      "comma-dangle": [
        "error",
        {
          "arrays": "always-multiline",
          "objects": "always-multiline",
          "imports": "always-multiline",
          "exports": "always-multiline",
          "functions": "never"
        }
      ]
    }
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
